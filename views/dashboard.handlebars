<button class="btn-primary">
  <a class="nav-link" href="/createPost">create post</a>
</button >

<button class="btn-primary">
  <a class="nav-link" id="logout" href="">Logout</a>
</button>



{{#each allPostsbyUser}}
<div class="card">
  <div class="card-body">
    <article class="post">
      <div class="d-flex justify-content-between">
        <h5 class="card-title"><a href="/post/{{id}}">{{title}}</a></h5>
        <p class="text-muted">Posted By {{'user.username'}} on {{format_date createdAt}}</p>
      </div>
      <p class="card-text">{{post_text}}</p>
    </article>
  </div>
</div>

{{/each}}
<script src="./js/logout.js"></script>